<h1>Listing team_compositions</h1>

<table>
  <tr>
    <th>Match</th>
    <th>Player1</th>
    <th>Player2</th>
    <th>Player3</th>
    <th>Player4</th>
    <th>Player5</th>
    <th>Player6</th>
    <th>Player7</th>
    <th>Player8</th>
    <th>Player9</th>
    <th>Player10</th>
    <th>Player11</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @team_compositions.each do |team_composition| %>
  <tr>
    <td><%= team_composition.match_id %></td>
    <td><%= team_composition.player1_id %></td>
    <td><%= team_composition.player2_id %></td>
    <td><%= team_composition.player3_id %></td>
    <td><%= team_composition.player4_id %></td>
    <td><%= team_composition.player5_id %></td>
    <td><%= team_composition.player6_id %></td>
    <td><%= team_composition.player7_id %></td>
    <td><%= team_composition.player8_id %></td>
    <td><%= team_composition.player9_id %></td>
    <td><%= team_composition.player10_id %></td>
    <td><%= team_composition.player11_id %></td>
    <td><%= link_to 'Show', team_composition %></td>
    <td><%= link_to 'Edit', edit_team_composition_path(team_composition) %></td>
    <td><%= link_to 'Destroy', team_composition, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

